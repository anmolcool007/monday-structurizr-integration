{
  "name": "monday-structurizr-integration",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "server": "npm run compile && nodemon ./dist/app.js",
    "server-with-debugger": "npm run compile && nodemon --delay 100ms --exec 'fuser -k 9228/tcp; node --inspect=0.0.0.0:9228 ./src/dist.js'",
    "expose": "ngrok http 8302",
    "stop": "kill-port 8302 && kill-port 4049 && kill-port 4040",
    "compile": "tsc"
  },
  "nodemonConfig": {
    "watch": [
      ".env",
      "src/"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.15",
    "proxyquire": "^2.0.1",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "chai": "^4.1.2",
    "concurrently": "^5.2.0",
    "cross-port-killer": "^1.2.1",
    "debug": "~2.6.9",
    "eslint": "^v6.8.0",
    "mocha": "^5.0.4",
    "mock-require": "^3.0.1",
    "ngrok": "^3.2.7",
    "nodemon": "^2.0.2",
    "nyc": "^12.0.2",
    "sequelize-cli": "^5.5.1",
    "sinon": "^7.4.2",
    "sqlite3": "^4.1.1",
    "typescript": "^4.0.5"
  },
  "author": {
    "name": "Dev Agrawal",
    "email": "agrawadv@mail.uc.edu"
  },
  "contributors": [
    {
      "name": "Anmol Gupta",
      "email": "gupta.28@iitj.ac.in"
    }
  ]
}
